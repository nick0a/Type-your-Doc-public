{
  "name": "mistral-document-classifier",
  "version": "1.0.0",
  "description": "Document classification system using Mistral OCR and Claude",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "ocr:random": "ts-node src/evaluation/runMistralOCRTest.ts --random",
    "ocr:pdf2img": "ts-node src/evaluation/runMistralOCRTest.ts --pdf-to-image",
    "ocr:image-url": "ts-node src/tools/runImageUrlOcr.ts",
    "ocr:image-url-custom": "ts-node src/tools/runImageUrlOcr.ts",
    "ocr:batch": "ts-node src/evaluation/runBatchOCRFromExisting.js"
  },
  "dependencies": {
    "@types/form-data": "^2.5.2",
    "axios": "^1.8.4",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.2",
    "fs-extra": "^11.1.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.0.375"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
